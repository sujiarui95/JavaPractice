void bark(String name,int size){
	if(size>10)
	System.out.println(name + " says HAH");
	else System.out.println("Wouwo~~");
}

void bark1(String name,int size){
	if(this.size>10)
	System.out.println(this.name + " says HAH");
	else System.out.println("Wouwo~~");
}


